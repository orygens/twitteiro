<%= auto_discovery_link_tag(:rss, url_options = {}, tag_options = {:title => "Twitteiro - Feed dos Usuários"}) %>
<h2> Usuários </h2>
<% @usuarios.each do |usuario| %>
  <tr>
    <td><%= usuario.name %></td>
    <td><%= usuario.web %></td>
    <td><%= usuario.bio %></td>
  </tr>
<% end %>
<%= render 'form' %>
